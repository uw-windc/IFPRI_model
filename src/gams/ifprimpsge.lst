 288  *       Extract these submatrices, setting elements of the SAM to zero
 289  *       as data are extracted:
 290   
 291  loop((g,s,r,c)$(bienes(r) and actividades(c) and mapr(r,g) and mapr(c,s)),
 292          id0(g,s) = sam(r,c); sam(r,c) = 0;
 293  );
 294   
 295  loop((f,s,r,c)$(factores(r) and actividades(c) and mapr(r,f) and mapr(c,s)),
 296          fd0(f,s) = sam(r,c); sam(r,c) = 0;
 297  );
 298  loop((h,f,r,c)$(hogares(r) and factores(c) and mapr(r,h) and mapr(c,f)),
 299          fe0(h,f) = sam(r,c); sam(r,c) = 0;
 300  );
 301  loop((f,r)$(factores(r) and mapr(r,f)),
 302          fe0("row",f) = -sam(r,"59"); sam(r,"59") = 0;
 303  );
 304   
 305  loop((s,h,r,c)$(actividades(r) and hogares(c) and mapr(r,s) and mapr(c,h)),
 306          cn0(s,h) = sam(r,c); sam(r,c) = 0;
 307  );
 308  sh0(s) = sum(h, cn0(s,h));
 309  loop((s,g,r,c)$(actividades(r) and bienes(c) and mapr(r,s) and mapr(c,g)),
 310          s0(s,g) = sam(r,c); sam(r,c) = 0;
 311  );
 312  loop((g,h,r,c)$(bienes(r) and hogares(c) and mapr(r,g) and mapr(c,h)),
 313          cm0(g,h) = sam(r,c); sam(r,c) = 0;
 314  );
 315  loop((g,r)$(bienes(r) and mapr(r,g)),
 316          gd0(g) = sam(r,"16"); sam(r,"16") = 0;
 317          gi0(g) = sam(r,"21"); sam(r,"21") = 0;
 318          gs0(g) = sam(r,"20"); sam(r,"20") = 0;
 319          i0(g) = sam(r,"22"); sam(r,"22") = 0;
 320          stk0(g) = sam(r,"23"); sam(r,"23") = 0;
 321          msd0(g) = sam(r,"54"); sam(r,"54") = 0;
 322          msm0(g) = sam(r,"55"); sam(r,"55") = 0;
 323          x0(g) = sam(r,"59"); sam(r,"59") = 0;
 324          m0(g) = sam("59",r); sam("59",r) = 0;
 325          tm0(g) = sam("58",r); sam("58",r) = 0;
 326          ta0(g) = sam("57",r); sam("57",r) = 0;
 327          mdd0(g) = sam("54",r); sam("54",r) = 0;
 328          mdm0(g) = sam("55",r); sam("55",r) = 0;
 329  );
