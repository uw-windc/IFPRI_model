 335  *       We have some tariff revenue for the SRV sector which
 336  *       has no imports.  Recategorize these data as indirect taxes:
 337   
 338  ta0(g)$(m0(g) = 0) = ta0(g) + tm0(g);
 339  tm0(g)$(m0(g) = 0) = 0;
 340   
 341  *       Domestic supply equals production less exports:
 342   
 343  d0(g) = sum(s,s0(s,g)) - x0(g);
 344   
 345  *       Aggregate domestic absorption:
 346   
 347  a0(g) = gd0(g) + gi0(g) + gs0(g) + i0(g) + stk0(g) + msd0(g) + msm0(g) + sum(s,id0(g,s)) + sum(h,cm0(g,h));
 348   
 349  parameter       profit(s,*)     Profit check for production and aggregate supply;
 350   
 351  profit(s,"y") = sh0(s) + sum(g, s0(s,g)) - sum(g, id0(g,s)) - sum(f, fd0(f,s));
 352  profit(g,"a") = a0(g) - m0(g) - d0(g) - tm0(g) - ta0(g) - mdd0(g) -mdm0(g);
 353  display profit;
 354   
 355  parameter       market  Market clearance;
 356   
 358   
 359  market(g,"s0") = sum(s,s0(s,g));        ! Sectoral supply
 360  market(g,"x0") = x0(g);                 ! Exports
 361  market(g,"m0") = m0(g);                 ! Imports (cif)
 362  market(g,"tm0") = tm0(g);               ! Tariff revenue
 363  market(g,"md") = mdd0(g)+mdm0(g);       ! Margin demand (domestic and imported goods)
 364  market(g,"ms") = msd0(g)+msm0(g);       ! Margin supply
 365  market(g,"ta0") = ta0(g);               ! Indirect taxes
 366  market(g,"id0") = sum(s, id0(g,s));     ! Intermediate demand
 367  market(g,"cm0") = sum(h, cm0(g,h));     ! Consumer demand (market)
 368  market(g,"i0") = i0(g);                 ! Investment
 369  market(g,"gd0") = gd0(g);               ! Government demand
 370  market(g,"gs0") = gs0(g);               ! Government social demand
 371  market(g,"gi0") = gi0(g);               ! Government investment demand
 372  market(g,"stk0") = stk0(g);             ! Changes in stock
 373   
 374  *       Generate a cross-check of consistency:
 375   
 376  market(g,"chk") = sum(s,s0(s,g))-x0(g) + m0(g) + tm0(g) + ta0(g) + mdd0(g)+mdm0(g)
 377          - sum(h, cm0(g,h)) - (msd0(g)+msm0(g)) - sum(s, id0(g,s)) - i0(g) - (gd0(g)+gs0(g)+gi0(g)) - stk0(g);
 378   
 379  *       Report the totals for each of these accounts:
 380   
 381  set acct /s0,m0,x0,tm0,md,ms,ta0,id0,cm0,i0,gs0,gd0,gi0,stk0/;
 382  market("total",acct) = sum(g,market(g,acct));
 383  display market;
